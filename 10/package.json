{
  "name": "cat-energy",
  "private": true,
  "description": "Личный проект «Sedona» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.2",
    "browser-sync": "2.27.7",
    "gulp": "4.0.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "postcss": "8.4.7",
    "postcss-csso": "6.0.0",
    "stylelint": "14.5.3"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"styles/style.css\"",
    "test": "npm run editorconfig && npm run stylelint",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "./*.json",
    "./*.js",
    "./*.html",
    "./scripts/**/*.js",
    "./images/**/*.svg",
    "./styles/style.css"
  ],
  "engines": {
    "node": "16.x"
  },
  "type": "module"
}
